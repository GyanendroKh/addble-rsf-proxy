{
  "name": "addble-panda-rsf-proxy",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "tsx watch --clear-screen=false ./src/main.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "docker:build": "docker build . -t gcr.io/preprod-panda-unified/panda-rsf-proxy-image --platform linux/amd64",
    "docker:deploy": "docker push gcr.io/preprod-panda-unified/panda-rsf-proxy-image && gcloud run deploy panda-rsf-proxy --image gcr.io/preprod-panda-unified/panda-rsf-proxy-image --project addble-market-test --region asia-south1"
  },
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "async-cache-dedupe": "^2.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-http-proxy": "^2.1.1",
    "tslib": "^2.8.1",
    "tweetnacl": "^1.0.3",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/express": "^5.0.1",
    "@types/express-http-proxy": "^1.6.6",
    "@types/node": "^22.14.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  }
}
